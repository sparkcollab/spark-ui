{
  "name": "spark-monorepo",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "packageManager": "npm@8.19.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "web": "yarn --cwd apps/web dev",
    "mobile": "yarn --cwd apps/mobile start"
  },
  "devDependencies": {
    "turbo": "^2.1.0"
  }
}
